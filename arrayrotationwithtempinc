#include <stdio.h>
#include <stdlib.h>
int arrrotate(int arr[], int size, int d){
    int temp[d];
    for(int i =0 ; i <= d;i++)
    {
        temp[i]=arr[i];
        for(int j =0; j< size-1;j++)
        {
            arr[j]= arr[j+1];
            
        }
        
        
        
        
    }
//    printf("%d \t\t\t\n",ssize);
    for(int q=0;q<=size-1;q++){
        
        printf("%d \n",arr[q]);
        //  printf("%d",temp[j]);
        //}
    }
    return 0;}



int main()
{

int arr[9]={9,4,2,1,5,3,6,7,0};
    int size = sizeof(arr)/sizeof(arr[0]);
int d;
scanf("%d",&d);
system("clear");
arrrotate(arr,size,d);


    return 0;
}
