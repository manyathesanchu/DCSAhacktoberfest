#include <stdio.h>


int arrrotate(int arr[], int size, int d){
    //int temp[20];
    for(int i =0 ; i <= d;i++)
    {
        int j,first;
        first = arr[0];
        for(j =0; j< size-1;j++)
        {
            arr[j]= arr[j+1];
        }
        arr[size-1]=first;
        
    }
    
    for(int q=0;q<=size-1;q++){
        
        printf("%d \n",arr[q]);
      //  printf("%d",temp[j]);
    //}
    }
    return 0;}


int main(){
    int size=9;
    int arr[9]={9,4,2,1,5,3,6,7,0};
    
    int d;
    scanf("%d",&d);
    
    arrrotate(arr,size,d);
}
